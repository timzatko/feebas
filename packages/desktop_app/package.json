{
    "name": "feebas-desktop-app",
    "description": "feebas-desktop-app",
    "author": "timzatko",
    "main": "main.js",
    "private": true,
    "version": "0.1.26",
    "repository": {
        "type": "git",
        "url": "https://github.com/timzatko/feebas"
    },
    "scripts": {
        "app:release": "sh ./scripts/release.sh",
        "build": "sh ./scripts/build.sh",
        "build:dev": "npm run build -- -c dev",
        "build:prod": "npm run build -- -c production",
        "electron:build": "npm run build:prod && electron-builder build",
        "electron:linux": "npm run build:prod && electron-builder build --linux",
        "electron:local": "npm run build:prod && electron .",
        "electron:mac": "npm run build:prod && electron-builder build --mac",
        "electron:serve-tsc": "tsc -p ./tsconfig-serve.json",
        "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve --config=$FEEBAS_DEV_CONFIG",
        "electron:windows": "npm run build:prod && electron-builder build --windows",
        "lint": "eslint src",
        "ng:serve": "ng serve",
        "ng": "ng",
        "postinstall": "node scripts/postinstall && electron-builder install-app-deps",
        "start": "npm run postinstall && npm-run-all -p ng:serve electron:serve",
        "start:fs-local": "cross-env FEEBAS_DEV_CONFIG=./integrations/fs-local/feebas.config.json npm run start",
        "start:fs-local-2": "cross-env FEEBAS_DEV_CONFIG=./integrations/fs-local-2/feebas.config.json npm run start",
        "tsc": "tsc"
    },
    "dependencies": {
        "@angular/animations": "^9.1.11",
        "@angular/cdk": "^9.2.4",
        "@angular/material": "^9.2.4",
        "@types/git-root-dir": "^1.0.0",
        "combined-stream": "^1.0.8",
        "electron-log": "^3.0.9",
        "fs-extra": "^7.0.1",
        "git-root-dir": "^1.0.2",
        "git-status": "^1.0.9",
        "hammerjs": "^2.0.8",
        "is-image": "^2.0.0",
        "klaw-sync": "^6.0.0",
        "lodash": "^4.17.15",
        "minimist": "^1.2.5",
        "pixelmatch": "^4.0.2",
        "pngjs": "^3.4.0",
        "request": "^2.88.2",
        "request-promise-native": "^1.0.8",
        "simple-git": "^1.132.0",
        "tslib": "^1.10.0",
        "unzipper": "^0.9.15"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.901.9",
        "@angular/cli": "9.1.9",
        "@angular/common": "9.1.11",
        "@angular/compiler": "9.1.11",
        "@angular/compiler-cli": "^9.1.11",
        "@angular/core": "9.1.11",
        "@angular/forms": "9.1.11",
        "@angular/http": "7.1.1",
        "@angular/language-service": "9.1.11",
        "@angular/platform-browser": "9.1.11",
        "@angular/platform-browser-dynamic": "9.1.11",
        "@angular/router": "9.1.11",
        "@ngx-translate/core": "11.0.1",
        "@ngx-translate/http-loader": "4.0.0",
        "@types/fs-extra": "^5.1.0",
        "@types/jasmine": "2.8.7",
        "@types/jasminewd2": "2.0.3",
        "@types/klaw-sync": "^6.0.0",
        "@types/node": "^12.11.1",
        "@types/pixelmatch": "^5.2.1",
        "@types/pngjs": "^3.4.2",
        "@types/request-promise-native": "^1.0.17",
        "@typescript-eslint/eslint-plugin": "^3.3.0",
        "@typescript-eslint/parser": "^3.3.0",
        "codelyzer": "^5.1.2",
        "core-js": "2.5.7",
        "cross-env": "^5.2.1",
        "electron": "^9.0.4",
        "electron-builder": "^22.7.0",
        "electron-reload": "^1.5.0",
        "eslint": "^7.3.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "install": "^0.13.0",
        "jasmine-core": "3.3.0",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "^5.1.0",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage-istanbul-reporter": "2.0.4",
        "karma-jasmine": "2.0.1",
        "karma-jasmine-html-reporter": "1.4.0",
        "npm": "^6.14.5",
        "npm-run-all": "4.1.5",
        "protractor": "^7.0.0",
        "rxjs": "6.5.3",
        "ts-node": "7.0.1",
        "typescript": "3.8.3",
        "wait-on": "3.2.0",
        "webdriver-manager": "12.1.0",
        "zone.js": "~0.10.2"
    }
}
