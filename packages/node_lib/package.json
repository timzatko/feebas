{
    "name": "feebas",
    "description": "Feebas is screenshot comparison tool which is intended to be used as a part of catching the visual regressions in end to end tests.",
    "main": "index.js",
    "author": "timzatko",
    "version": "0.1.10",
    "license": "MIT",
    "keywords": [
        "screenshot",
        "test",
        "visual regression",
        "e2e",
        "ci",
        "compare"
    ],
    "desktop_app-version": "0.1.10",
    "desktop_app-fileName": "feebas-desktop-app",
    "repository": {
        "type": "git",
        "url": "https://github.com/timzatko/feebas"
    },
    "bin": {
        "feebas": "cli.js"
    },
    "scripts": {
        "start": "./cli.js",
        "dev": "npm run app:build && node scripts/install.js --dev && npm run start --",
        "dev:fs-local": "npm run dev -- --config=../desktop_app/integrations/fs-local/feebas.config.json",
        "app:build": "cd ../desktop_app && npm run electron:build",
        "postinstall": "node ./scripts/install",
        "prepublishOnly": "node ./scripts/pre-publish"
    },
    "dependencies": {
        "cli-progress": "^2.1.1",
        "fs-extra": "^7.0.1",
        "meow": "^5.0.0",
        "node-7z-forall": "^1.0.0",
        "request": "^2.88.0",
        "tempfile": "^2.0.0"
    },
    "devDependencies": {
        "ncp": "^2.0.0"
    }
}
